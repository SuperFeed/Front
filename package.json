{
  "name": "Front",
  "version": "0.0.0",
  "author": "Ryan Delaney <rrdelaney@outlook.com>",
  "repository": "https://github.com/SuperFeed/Front",
  "license": "MIT",
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "test/*"
    ],
    "globals": [
      "fetch",
      "FormData"
    ]
  },
  "scripts": {
    "test": "standard | snazzy",
    "dev-api": "cross-env BABEL_ENV=lambda nodemon --watch functions run-api.js",
    "dev-client": "webpack-dev-server --history-api-fallback --hot --inline --progress --colors --port 3000",
    "build-client": "cross-env NODE_ENV=production webpack",
    "predev-server": "cross-env BABEL_ENV=server babel src -d _server",
    "dev-server": "cross-env BABEL_ENV=server parallelshell \"babel --watch src -d _server\" \"nodemon --watch _server _server/server.js\"",
    "build-server": "BABEL_ENV=server babel src -d target",
    "predev": "rimraf _client _server",
    "dev": "parallelshell \"npm run dev-client\" \"npm run dev-server\" \"npm run dev-api\"",
    "prebuild": "rimraf target && mkdirp target",
    "build": "npm run build-server && npm run build-client",
    "postbuild": "cp package.json target && cp -r resources/. target"
  },
  "devDependencies": {
    "apex.js": "^1.1.0",
    "ava": "^0.13.0",
    "babel-cli": "^6.6.5",
    "babel-core": "^6.5.2",
    "babel-eslint": "^5.0.0",
    "babel-loader": "^6.2.3",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.5.2",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.7.2",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.9.0",
    "html-webpack-template": "^4.0.0",
    "json-loader": "^0.5.4",
    "mkdirp": "^0.5.1",
    "nodemon": "^1.9.0",
    "parallelshell": "^2.0.0",
    "rimraf": "^2.5.2",
    "snazzy": "^3.0.0",
    "standard": "^6.0.8",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.13",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.5.0",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-facebook-login": "^2.1.3",
    "react-redux": "^4.4.0",
    "react-router": "^2.0.1",
    "redux": "^3.3.1",
    "redux-actions": "^0.9.1",
    "redux-devtools": "^3.1.1",
    "redux-devtools-dock-monitor": "^1.0.1",
    "redux-devtools-log-monitor": "^1.0.4",
    "redux-promise": "^0.5.1",
    "redux-reaction": "0.0.1",
    "reselect": "^2.2.1",
    "soular": "^0.3.6"
  }
}
